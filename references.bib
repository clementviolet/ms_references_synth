	Eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/oik.05670},
	Journal = {Oikos},
	Keywords = {Network analysis, Indirect interactions, Part 3},
	Number = {2},
	Pages = {154-170},
	Title = {Motifs in bipartite ecological networks: uncovering indirect interactions},
